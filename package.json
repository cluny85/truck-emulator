{
  "name": "truck-emulator",
  "version": "0.0.1",
  "author": "Bob.io",
  "license": "Apache",
  "scripts": {
    "lint": "eslint --ext .js .",
    "start": "nodemon --watch .",
    "debug": "node --inspect=0.0.0.0:9229 .",
    "start:dev": "pm2 start ecosystem.config.js",
    "start:pro": "NODE_ENV=production node ./node_modules/pm2/bin/pm2 start index.js --name api.bob.io",
    "test": "./node_modules/.bin/nyc mocha -R spec 'src/**/*.test.js' --exit",
    "test:dev": "NODE_ENV=development mocha --recursive spec 'src/**/*.test.js' --exit",
    "test:pre": "NODE_ENV=preproduction mocha -R spec 'src/**/*.test.js' --exit",
    "test:pro": "NODE_ENV=production mocha -R spec 'src/**/*.test.js' --exit",
    "test:html": "./node_modules/.bin/nyc --reporter=html mocha -R spec 'src/**/*.test.js' --exit",
    "gen:coverage": "./node_modules/.bin/nyc --reporter=lcov --reporter=text-lcov mocha -R spec 'src/**/*.test.js' --exit && ./node_modules/.bin/coveralls"
  },
  "env": [
    "mocha"
  ],
  "dependencies": {
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.23",
    "socket.io-client": "^2.2.0",
    "uniqid": "^5.0.3",
    "ws": "^6.1.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.2.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.7.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "node-mocks-http": "^1.7.3",
    "nodemon": "^1.18.5",
    "nyc": "^12.0.2",
    "proxyquire": "^2.1.0",
    "sinon": "^6.3.5",
    "socket.io": "^2.2.0"
  }
}
